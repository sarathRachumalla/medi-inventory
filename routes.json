{
    "routes": [
        {
            "path": "landing",
            "component": "landingComponent",
            "canActivate": ""
        },
        {
            "path": "home",
            "component": "homeComponent",
            "canActivate": "",
            "children": [
                {
                    "path": "hospitals",
                    "component": "hospitalsComponent"
                },
                {
                    "path": "inventory",
                    "component": "inventoryComponent"
                }
            ]
        },
        {
            "path": "''",
            "redirectTo": "home",
            "pathMatch": "full",
            "default": true
        },
        {
            "path": "**",
            "component": "PageNotFoundComponent"
        }
    ]
}
